    "@types/node-fetch": "^2.6.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "nock": "^13.4.0",
    "openapi-fetch": "^0.14.0",
    "openapi-typescript": "^7.8.0",
    "openapi-typescript-codegen": "^0.29.0",
